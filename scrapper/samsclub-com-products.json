{
    "_id": "samsclub-com-products",
    "startUrl": [
        "https://www.samsclub.com/b/ground-coffee/1494"
    ],
    "selectors": [
        {
            "id": "results-count",
            "multiple": false,
            "parentSelectors": [
                "_root"
            ],
            "regex": "",
            "selector": "div.sc-page-title-results-total",
            "type": "SelectorText"
        },
        {
            "id": "pagination",
            "paginationType": "clickMore",
            "parentSelectors": [
                "_root",
                "pagination"
            ],
            "selector": ".sc-pagination-next button, button[aria-label=\"next page\"]",
            "type": "SelectorPagination"
        },
        {
            "id": "product-element",
            "multiple": true,
            "parentSelectors": [
                "pagination"
            ],
            "selector": ".sc-plp-cards-card",
            "type": "SelectorElement"
        },
        {
            "id": "product-link",
            "multiple": true,
            "parentSelectors": [
                "product-element"
            ],
            "selector": "a[href*='/p/']",
            "type": "SelectorLink"
        },
        {
            "id": "product-page",
            "multiple": true,
            "parentSelectors": [
                "product-link"
            ],
            "selector": ":has(:contains('Item #').sc-product-subtitle-text)html",
            "type": "SelectorElement"
        },
        {
            "id": "product-name",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "regex": "",
            "selector": "h1",
            "type": "SelectorText"
        },
        {
            "id": "product-item-number",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "regex": "",
            "selector": ":contains('Item #').sc-product-subtitle-text",
            "type": "SelectorText"
        },
        {
            "id": "product-model-number",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "regex": "",
            "selector": ":contains('Model #').sc-product-subtitle-text",
            "type": "SelectorText"
        },
        {
            "id": "product-brand",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "regex": "",
            "selector": ":contains('By ').sc-product-subtitle-text",
            "type": "SelectorText"
        },
        {
            "id": "product-price",
            "multiple": false,
            "parentSelectors": [
                "product-element"
            ],
            "regex": "",
            "selector": ".sc-price .visuallyhidden, .sc-pc-map-price, :contains('$0').sc-pc-medium-desktop-moneybox .sc-pc-select-club-message",
            "type": "SelectorText"
        },
        {
            "id": "product-rating",
            "multiple": false,
            "parentSelectors": [
                "review-data-wrapper"
            ],
            "regex": "",
            "selector": "[itemprop=\"ratingValue\"]",
            "type": "SelectorText"
        },
        {
            "id": "product-review-count",
            "multiple": false,
            "parentSelectors": [
                "review-data-wrapper"
            ],
            "regex": "",
            "selector": "[itemprop=\"reviewCount\"]",
            "type": "SelectorText"
        },
        {
            "id": "product-highlights",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "regex": "",
            "selector": ".sc-pc-about-this-item div",
            "type": "SelectorText"
        },
        {
            "id": "product-description",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "regex": "",
            "selector": ".sc-description-about-long, .sc-l-description-about-long",
            "type": "SelectorText"
        },
        {
            "id": "product-specifications",
            "multiple": false,
            "parentSelectors": [
                "specifications-click"
            ],
            "regex": "",
            "selector": "div.sc-description-about-full-spec",
            "type": "SelectorText"
        },
        {
            "extractAttribute": "content",
            "id": "product-image-src",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "selector": "meta[property='og:image']",
            "type": "SelectorElementAttribute"
        },
        {
            "extractAttribute": "src",
            "id": "product-images-grouped",
            "parentSelectors": [
                "product-page"
            ],
            "selector": "img[class*='-child-image-thumb']",
            "type": "SelectorGroup"
        },
        {
            "extractAttribute": "",
            "id": "product-category",
            "parentSelectors": [
                "product-page"
            ],
            "selector": "[class*='breadcrumbs'] li:nth-of-type(n+2)",
            "type": "SelectorGroup"
        },
        {
            "clickElementSelector": "button.sc-description-about-tab:contains('Specifications')",
            "clickElementUniquenessType": "uniqueCSSSelector",
            "clickType": "clickOnce",
            "delay": 0,
            "discardInitialElements": "discard-when-click-element-exists",
            "id": "specifications-click",
            "multiple": true,
            "parentSelectors": [
                "product-page"
            ],
            "selector": "_parent_",
            "type": "SelectorElementClick"
        },
        {
            "id": "review-data-wrapper",
            "multiple": false,
            "parentSelectors": [
                "product-page"
            ],
            "selector": "div[itemprop='aggregateRating']",
            "type": "SelectorElement"
        }
    ],
    "websiteStateSetup": {
        "enabled": true,
        "performWhenNotFoundSelector": "[class=\"club-name\"]:contains(\"Secaucus\")",
        "actions": [
            {
                "type": "openUrl",
                "url": "https://www.samsclub.com/club/secaucus-nj-sams-club/4774"
            },
            {
                "selector": "button.bst-btn-medium.blue-steel-button",
                "type": "click"
            }
        ]
    }
}