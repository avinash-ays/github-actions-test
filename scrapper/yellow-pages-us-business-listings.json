{
    "_id": "yellow-pages-us-business-listings",
    "startUrl": [
        "https://www.yellowpages.com/cleveland-oh/martial-arts-instruction"
    ],
    "selectors": [
        {
            "id": "listing-pagination",
            "paginationType": "auto",
            "parentSelectors": [
                "_root",
                "listing-pagination"
            ],
            "selector": "a.next",
            "type": "SelectorPagination"
        },
        {
            "id": "listing-elem",
            "multiple": true,
            "parentSelectors": [
                "listing-pagination"
            ],
            "selector": ".result div.srp-listing",
            "type": "SelectorElement"
        },
        {
            "id": "listing-url",
            "multiple": true,
            "parentSelectors": [
                "listing-elem"
            ],
            "selector": "a.business-name",
            "type": "SelectorLink"
        },
        {
            "id": "listing-title",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "regex": "",
            "selector": ".business-name",
            "type": "SelectorText"
        },
        {
            "id": "listing-categories",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "regex": "",
            "selector": "div.categories",
            "type": "SelectorText"
        },
        {
            "id": "listing-street-address",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "regex": "",
            "selector": "div.street-address",
            "type": "SelectorText"
        },
        {
            "id": "listing-locality",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "regex": "",
            "selector": "div.locality",
            "type": "SelectorText"
        },
        {
            "extractAttribute": "href",
            "id": "listing-website",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "selector": "a.track-visit-website",
            "type": "SelectorElementAttribute"
        },
        {
            "id": "listing-phone",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "regex": "",
            "selector": "div.phones",
            "type": "SelectorText"
        },
        {
            "extractAttribute": "class",
            "id": "listing-rating",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "selector": ".result-rating",
            "type": "SelectorElementAttribute"
        },
        {
            "id": "listing-review-count",
            "multiple": false,
            "parentSelectors": [
                "listing-elem"
            ],
            "regex": "",
            "selector": ".rating .count",
            "type": "SelectorText"
        },
        {
            "id": "business",
            "multiple": true,
            "parentSelectors": [
                "listing-url"
            ],
            "selector": "body:has(article.business-card)",
            "type": "SelectorElement"
        },
        {
            "id": "listing-categories-all",
            "multiple": false,
            "parentSelectors": [
                "business"
            ],
            "regex": "",
            "selector": "dd.categories",
            "type": "SelectorText"
        },
        {
            "id": "description",
            "multiple": false,
            "parentSelectors": [
                "business"
            ],
            "regex": "",
            "selector": "dd.general-info",
            "type": "SelectorText"
        },
        {
            "id": "opening-hours",
            "multiple": false,
            "parentSelectors": [
                "business"
            ],
            "regex": "",
            "selector": "dd.open-hours",
            "type": "SelectorHTML"
        },
        {
            "id": "payment-methods",
            "multiple": false,
            "parentSelectors": [
                "business"
            ],
            "regex": "",
            "selector": "dd.payment",
            "type": "SelectorText"
        },
        {
            "extractAttribute": "href",
            "id": "email",
            "multiple": false,
            "parentSelectors": [
                "business"
            ],
            "selector": "a.email-business",
            "type": "SelectorElementAttribute"
        }
    ]
}